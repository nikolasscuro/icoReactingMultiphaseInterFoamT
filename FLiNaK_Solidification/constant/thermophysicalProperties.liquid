FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       88.5;
    }
    equationOfState
    {
        beta            1.912e-4;
        rho0            2200;
        T0              726;
    }
    thermodynamics
    {
        Cp              2100;
        Hf              0;         //Hf          -1.5879e+07
    }
    transport
    {
        mu          0.008;   //dynamic viscosity
        Pr          13.7;
    }
}


