FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties.liquid;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState Boussinesq;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles          1;
        molWeight       88.52;
    }
    equationOfState
    {
        beta            1.912e-4;
        rho0            2150;
        T0              726;
    }
    thermodynamics
    {
        Cp              1750;
        Hf              0;
    }
    transport
    {
        mu          0.01;   //dynamic viscosity
        Pr          21.9;
    }
}


