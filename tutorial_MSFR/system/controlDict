// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     icoReactingMultiphaseInterFoamT2;

startFrom       startTime;

startTime       100;

stopAt          endTime;

endTime         3000;

deltaT          0.05;

writeControl    adjustable;

writeInterval   10;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable no;

adjustTimeStep  no;

maxDeltaT       1;

maxCo           1.;

maxAlphaCo      1.;

maxAlphaDdt     2.3;

maxDi           25;


